const body=document.body,hamburger=document.querySelector("#hamburger"),navHasChildItems=document.querySelectorAll(".primary-nav__list .primary-nav__list-item.has-children");hamburger.addEventListener("click",()=>{body.classList.toggle("offscreen-is-active")});navHasChildItems.forEach(n=>{n.addEventListener("click",function(){screen.width<1080&&this.classList.toggle("toggle-is-open")})});window.onload=()=>{const t=document.getElementsByTagName("head")[0],n=document.createElement("link");n.rel="stylesheet";n.type="text/css";n.href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.3.0/css/all.min.css";t.appendChild(n)}